{
  "name": "nuxt-pages-plus",
  "version": "1.0.0-beta.2",
  "description": "A Nuxt module that enables complex routing for Nuxt Pages.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serkodev/nuxt-pages-plus.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev",
    "dev:build": "nuxi build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare examples/base-utils && nuxi prepare examples/base",
    "play": "nuxi dev playground",
    "play:build": "nuxi build playground",
    "docs:dev": "pnpm --dir docs dev",
    "docs:build": "pnpm --dir docs build",
    "docs:deploy": "pnpm dev:prepare && pnpm docs:build",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build && bumpp",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare:types": "pnpm --filter \"./examples/**\" --filter \"./playground\" prepare:types",
    "prepare": "nuxt-module-build prepare"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:nuxt",
    "@vueuse/core": "^13.9.0",
    "defu": "^6.1.4",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@nuxt/eslint-config": "catalog:dev",
    "@nuxt/module-builder": "catalog:nuxt",
    "@nuxt/schema": "catalog:nuxt",
    "@nuxt/test-utils": "catalog:nuxt",
    "@types/node": "^22.9.0",
    "bumpp": "^10.2.3",
    "eslint": "catalog:dev",
    "nuxt": "catalog:nuxt",
    "typescript": "catalog:dev",
    "vitest": "^3.2.4",
    "vue-router": "catalog:"
  },
  "peerDependencies": {
    "nuxt": ">=3.16.0"
  },
  "packageManager": "pnpm@10.17.1"
}
